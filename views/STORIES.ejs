    @media screen and (max-width: 600px) {
        .topnav.responsive {
            padding: 0%;
        }
        /* .topnav.responsive .icon {
      float: right;
    position: absolute;
    right: 0;
    top: 0;
  } */
        .topnav.responsive a {
            float: none;
            display: flex;
            text-align: center;
            padding: 0%;
        }
    }
    
</style>

<body>
    <header>
        <div class="topnav" id="myTopnav">
